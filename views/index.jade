                        a(style="color: #ffc750b3;") MCC&MNC Search
                        a.jg.btn.btn-outline-light.float-right(type='button', id="MCC_MNC_selectBtn_issue") 확인
                        a.jg.btn.btn-outline-light.float-left(type='button', id="MCC_MNC_cancelBtn_issue") 취소
                      .card-body.p-0
                        .table-responsive(style='width: 100%; height:285px; overflow-y:auto; overflow-x: hidden;')
                          table.table
                            thead.bg-light
                              tr.border-0
                                th.border-0 Check
                                th.border-0 Country
                                th.border-0 Operator
                                th.border-0 MCC
                                th.border-0 MNC
                            tbody.search(id='totSearch')
                              -each val in rows_all
                                include MCC_MNC_search_tbl_issue.jade
