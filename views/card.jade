        .roaming-card-body(style="align-items: center;vertical-align: middle;line-hight:30%;")
            .country_name
                h3(style="color:#ffcc00") #{val.country_name}
            .operator_name
                h3(style="display: block;color:#ff9900") #{val.operator_name}
            .row(style="margin-top:20px;margin-bottom:5px")
                .col-xl-6.col-12
                    .subs_count
                        h1.text-light #{val.subs_count_string}
                    h6.text-muted LTE 
                .col-xl-6.col-12
                    .subs_count
                        h1.text-light #{val.subs_count_string}
